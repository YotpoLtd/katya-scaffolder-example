            {
              "fields": [
                {
                  "default": "",
                  "type": "text"
                },
                {
                  "attribute": "welcome-text",
                  "name": "Welcome text",
                  "type": "dropdown",
                  "default": "Good morning",
                  "alertMessage": {
                    "alertTitle": "You've changed your widget welcome text(it's an example customization value)",
                    "alertSubtitle": "Please make sure to delete any duplicate code you might have on your store.",
                    "status": "warning",
                    "icon": true
                  },
                  "constraints": {
                    "validValues": [
                      {
                        "display": "Good morning",
                        "value": "Good morning"
                      },
                      {
                        "display": "Good night",
                        "value": "good night"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "name": "COLORS",
              "fields": [
